<UserControl x:Class="SpaceMarines.ucSmallBullet"
    xmlns:my="clr-namespace:SpaceMarines"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" Foreground="{x:Null}" BorderBrush="Black" Style="{StaticResource EntityControlStyle}">
    
    <Grid x:Name="LayoutRoot">
        <Ellipse HorizontalAlignment="Left"  Name="ellipse1"  VerticalAlignment="Top" Width="8" Height="8" Fill="#FFC4FEFF"/>
    </Grid>
</UserControl>
